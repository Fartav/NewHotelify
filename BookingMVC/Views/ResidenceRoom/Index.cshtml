@model IEnumerable<Booking.Models.ResidenceRoom>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Capacity)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HasDinner)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HasBreakfast)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HasLunch)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreateDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModifyDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeleteDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreateDateTicks)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ModifyDateTicks)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DeleteDateTicks)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsDeleted)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Capacity)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HasDinner)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HasBreakfast)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HasLunch)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifyDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeleteDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreateDateTicks)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModifyDateTicks)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DeleteDateTicks)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsDeleted)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
